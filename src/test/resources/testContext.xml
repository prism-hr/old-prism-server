<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
		http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-3.0.xsd">

	<import resource="testHibernateContext.xml" />
    
    <import resource="../../src/main/webapp/WEB-INF/spring/application-context.xml" /> 
     
    <bean id="defaultTransactionTemplate" class="org.easymock.EasyMock" factory-method="createMock" primary="true">
        <constructor-arg value="org.springframework.transaction.support.TransactionTemplate" />
    </bean>
    
    <bean id="uclExportServiceApplicationInitializer" class="org.easymock.EasyMock" factory-method="createMock" primary="true">
        <constructor-arg value="com.zuehlke.pgadmissions.lifecycle.UclExportServiceApplicationInitializer" />
    </bean>
    
</beans>
